{
  "name": "cloud-racket-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK Infrastructure for Cloud Racket Platform",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "synth:dev": "cdk synth --context env=dev",
    "synth:staging": "cdk synth --context env=staging",
    "synth:prod": "cdk synth --context env=prod",
    "diff": "cdk diff",
    "diff:dev": "cdk diff --context env=dev",
    "diff:staging": "cdk diff --context env=staging",
    "diff:prod": "cdk diff --context env=prod",
    "deploy": "cdk deploy --all",
    "deploy:dev": "cdk deploy --all --context env=dev",
    "deploy:staging": "cdk deploy --all --context env=staging --require-approval broadening",
    "deploy:prod": "cdk deploy --all --context env=prod --require-approval never",
    "destroy": "cdk destroy --all",
    "destroy:dev": "cdk destroy --all --context env=dev",
    "destroy:staging": "cdk destroy --all --context env=staging",
    "destroy:prod": "cdk destroy --all --context env=prod",
    "bootstrap": "cdk bootstrap",
    "bootstrap:dev": "cdk bootstrap --context env=dev",
    "bootstrap:staging": "cdk bootstrap --context env=staging",
    "bootstrap:prod": "cdk bootstrap --context env=prod"
  },
  "keywords": ["aws", "cdk", "infrastructure", "serverless"],
  "author": "Cloud Racket Team",
  "license": "MIT",
  "dependencies": {
    "aws-cdk-lib": "^2.110.0",
    "constructs": "^10.3.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "aws-cdk": "^2.110.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
}
